FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      T;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //
#include "../../nozzleDict"

dimensions      [0 0 0 1 0 0 0];

internalField   uniform $Tatm;

boundaryField
{
    jetInlet
    {
        type            inletOutlet;
        value           $internalField;
        inletValue      uniform $Tatm;
    }
    jetOutlet
    {
        type         uniformFixedValue;
        uniformValue        table ( (0 $Tatm) (50 $Tjet) );
    }
    airInlet
    {
        type            outletInlet;
        value           uniform $Tatm;
        outletValue      uniform $Tatm;
    }
    airOutlet
    {
        type            inletOutlet;
        value           $internalField;
        inletValue      uniform $Tatm;
    }
    walls
    {
        type            zeroGradient;
    }
    fastWalls
    {
        type            zeroGradient;
    }
    top
    {
        type            outletInlet;
        value           uniform $Tatm;
        outletValue     uniform $Tatm;
    }
    axis
    {
        type            empty;
    }
    sideA
    {
        type            wedge;
    }
    sideB
    {
        type            wedge;
    }
}

