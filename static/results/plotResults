set term png size 1200,600
set output 'batchPlot.png'

set multiplot layout 1,2

#set size 1.0,1.0
set xlabel 'Inner Radius (m)'
plot "batchResults.dat" every ::1 using 1:2 with lp title 'bypass ratio'

#set size 1.0,1.0
plot "batchResults.dat" every ::1 using 1:3 with lp title 'thrust ratio'

unset multiplot
